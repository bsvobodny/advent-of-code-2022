{
  "name": "advent-of-code-2022",
  "version": "1.0.0",
  "description": "https://adventofcode.com/ ",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest --coverage"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "days/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bsvobodny/advent-of-code-2022.git"
  },
  "keywords": [
    "advent",
    "code"
  ],
  "author": "Benjamin Svobodny",
  "license": "ISC",
  "devDependencies": {
    "big.js": "^6.2.1",
    "jest": "^29.3.1"
  }
}
